def estimate_gaussian(X): 
    """
    Calculates mean and variance of all features 
    in the dataset
    """
    m, n = X.shape
    
    # Mean of each feature
    mu = np.sum(X, axis=0) / m
    
    # Variance of each feature
    var = np.sum((X - mu) ** 2, axis=0) / m
        
    return mu, var
